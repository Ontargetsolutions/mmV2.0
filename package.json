{
  "name": "montagemosaics",
  "version": "1.0.0",
  "description": "online app to quote mosaics",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ontargetsolutions/mmV2.0.git"
  },
  "author": "Irina Machado",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ontargetsolutions/mmV2.0/issues"
  },
  "homepage": "https://github.com/Ontargetsolutions/mmV2.0#readme",
  "dependencies": {
    "authorizenet": "^1.0.7",
    "aws-sdk": "^2.645.0",
    "concurrently": "^4.1.2",
    "country-code-lookup": "0.0.17",
    "countrystatesjs": "0.0.12",
    "dotenv": "^8.2.0",
    "emailjs-com": "^2.4.1",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "moment": "^2.29.2",
    "moment-timezone": "^0.5.27",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "mysql2": "^2.1.0",
    "node-sass": "^4.13.1",
    "nodemailer": "^6.4.10",
    "nodemon": "^1.18.7",
    "request-promise": "^4.2.5",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1"
  }
}
